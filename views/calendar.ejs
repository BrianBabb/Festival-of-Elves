<!DOCTYPE html>

<head>
  <link rel="stylesheet" href="/styles/styles.css">
  <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.4.1/jquery.min.js"></script>
</head>

<body>
  <div class="container">
    <div class="calendar">
      <!--front div start here-->
      <div class="front">
        <div class="current-date">
          <!-- <h1>Friday 15th</h1> -->
          <h1>December 2019</h1>
        </div>
        <!--current month div start-->
        <div class="current-month">
          <ul class="week-days">
            <li>SUN</li>
            <li>MON</li>
            <li>TUE</li>
            <li>WED</li>
            <li>THU</li>
            <li>FRI</li>
            <li>SAT</li>
          </ul>

          <div class="weeks">
            <!--weeks here-->
            <div class="first">
              <span id="0" value="1">01</span>
              <span id="1">02</span>
              <span id="2">03</span>
              <span id="3">04</span>
              <span id="4">05</span>
              <span id="5">06</span>
              <span id="6">07</span>
            </div>
            <div class="second">
              <span id="7">08</span>
              <span id="8">09</span>
              <span id="9">10</span>
              <span id="10">11</span>
              <span id="11">12</span>
              <span id="12">13</span>
              <span id="13">14</span>
            </div>

            <div class="third">
              <span id="14">15</span>
              <span id="15">16</span>
              <span id="16">17</span>
              <span id="17">18</span>
              <span id="18">19</span>
              <span id="19">20</span>
              <span id="20">21</span>
            </div>

            <div class="fourth">
              <span id="21">22</span>
              <span id="22">23</span>
              <span id="23">24</span>
              <span id="24">25</span>
              <span id="25">26</span>
              <span >27</span>
              <span >28</span>
            </div>

            <div class="fifth">
              <span>29</span>
              <span>30</span>
              <span>31</span>
              <span class="last-month">1</span>
              <span class="last-month">2</span>
              <span class="last-month">3</span>
              <span class="last-month">4</span>
            </div>
          </div>
        </div>
        <!--current month closed-->
      </div>
      <!--front div closed-->

      <div class="back">
        <div class="info">
          <div class="address">
            <div>
              <p>Dear <%= firstchild %>
                and <%= secondchild %>,</p>
            </div>
            <div>
              <p id="msg"></p>
            </div>
            <div>
              <p>Love, </p>
                <p><%= firstelf %>
                and <%= secondelf %>.</p>
            </div>

            <!-- <p id="demo"></p> -->
            </p>


            <div class="actions">
              <button class="save">
                Save <i class="ion-checkmark"></i>
              </button>
              <button class="dismiss">
                Dismiss <i class="ion-android-close"></i>
              </button>
            </div>
          </div>
          <!--div back-->
        </div>
        <!--calendar div closed here-->

        <div class="actions">
          <div class="container">
            <a class="btn btn-danger btn-lg" href="/logout" role="button">Log Out</a>

          </div>
        </div>
      </div>
      <!--container div-->


      <!-- <a class="btn btn-light btn-lg" href="/logout" role="button">Log Out</a> -->
      <script src="/js/index.js"></script>


</body>

</html>